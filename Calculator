#include <iostream>

using namespace std;


int addTwoNumbers(int num_a, int num_b)
{
	int sum = num_a + num_b;

	return sum;
}

int Multiple(int a, int b)
{
	return a* b;
}

void HelloWorld()
{
	cout << "Hello World!" << endl;
}
int main()
{
	cout << addTwoNumbers(1, 3) << endl;
	cout << 1 + 3 << endl;
	HelloWorld();
	Multiple(1, 2);

	return 0;
}

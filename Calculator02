#include <iostream>

using namespace std;


int Add(int num_a, int num_b)
{
	int sum = num_a + num_b;

	return sum;
}

int Multiple(int a, int b)
{
	return a* b;
}

int Divide(int a, int b)
{
	return a / b;
}

int Minus(int a, int b)
{
	return a - b;
}

void HelloWorld()
{
	cout << "Hello World!" << endl;
}
int main()
{
	cout << Add(1, 3) << endl;
	cout << Minus(2, 1) << endl;
	cout << Multiple(2, 3) << endl;
	cout << Divide(6, 3) << endl;
